[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
C=US
ST=MA
L=Cambridge
O=Hindsight
OU=John
emailAddress=john@gmail.com
CN = PYTHON_CONFIG_INSERT_IP_HERE

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = PYTHON_CONFIG_INSERT_IP_HERE

[ v3_ca ]
subjectAltName = @alt_names
